<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/new_orange_bgm_color" />
            <gradient android:angle="-90" android:startColor="@color/new_orange_bgm_color" android:endColor="@color/new_orange_bgm_color"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/new_orange_bgm_color" />
            <solid android:color="@color/new_orange_bgm_color"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/new_orange_bgm_color" />
            <gradient android:angle="-90" android:startColor="@color/new_orange_bgm_color" android:endColor="@color/new_orange_bgm_color" />
        </shape>
    </item>
</selector>